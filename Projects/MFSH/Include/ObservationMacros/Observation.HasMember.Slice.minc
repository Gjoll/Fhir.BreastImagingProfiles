/*
 Slice Observation.hasMember.
 */
#macro once Observation.HasMember.Slice()
  #apply Graph.NodeElement("hasMember", "HasMembers")
  #apply Graph.LinkByName("focus", "^%Id%/$", "^%Id%/hasMember/$", "0")

  #incompatible Observation.LeafFragment
  * hasMember ^slicing.discriminator.type = #value
  * hasMember ^slicing.discriminator.path = "url"
  * hasMember ^slicing.rules = #open
  * hasMember ^slicing.ordered = false
  * hasMember ^slicing.description = "Component slicing"
#end
