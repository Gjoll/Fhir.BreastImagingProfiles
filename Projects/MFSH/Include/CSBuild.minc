//
// Define a CS Class.
//
#macro CSBuild.DefineClass() > "%CSBuildDir%\\%BaseDir%\\%BaseNameNoExtension%.csBuild"
  `"defineClass" : {
  `    "className" : "%Id%"
  `  },
#end

//
// Define an CS Interface.
//
#macro CSBuild.DefineInterface() > "%CSBuildDir%\\%BaseDir%\\%BaseNameNoExtension%.csBuild"
  `"defineInterface" : {
  `    "interfaceName" : "%Id%"
  `  },
#end