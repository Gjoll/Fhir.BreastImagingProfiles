/*
 Resource.FixCode
 Fix resource.code to passed value.
 */
 
#macro Resource.Fix(%Element%, %Code%, %DisplayName%)
  * %Element% = %Code%
  
  // -----------------------------------------
  // Graph Macros
  // -----------------------------------------
  #use Graph
    #apply LinkByName("focus", "^%ProfileId%/$", "^%ProfileId%/%Element%", "0", "Cmp")
    #apply NodeElement("1", "%Element%", "%DisplayName%")
    #apply LinkByBinding("focus", "^%ProfileId%.%Element%", "", "Cmp")
  #end  
#end
