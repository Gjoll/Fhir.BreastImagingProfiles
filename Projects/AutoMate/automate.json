{
    "traceFlag": false,
	"watchs": [
		{
			"name": "MFSH",
			"watchPaths": ["..\\MFSH"],
			"commands": [
				{
  			        "workingDir": "..",
					"cmdPath": "MFSH.exe",
					"cmdArgs": "AutoMate\\mfsh.json"
				}
			]
		},
		{
			"name": "FSH",
			"watchPaths": ["..\\FSH"],
			"commands": [
				{
  			        "workingDir": "..",
					"cmdPath": "%ApplicationData%\\npm\\sushi.cmd",
					"cmdArgs": "FSH --out Build"
				}
			]
		},
		{
			"name": "FGraph",
			"watchPaths": ["..\\FSH", "..\\GraphFiles"],
			"commands": [
				{
  			        "workingDir": "..",
					"cmdPath": "FGraph",
					"cmdArgs": "AutoMate\\fgraph.json"
				},
				{
  			        "workingDir": "..",
					"cmdPath": "IGBuilder",
					"cmdArgs": "AutoMate\\igbuilder.json"
				}
			]
		}
	]
}