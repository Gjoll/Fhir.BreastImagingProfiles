{
	"baseInputDir": "MFSH",
	"baseOutputDir": "FSH",
	"fragDir": "FSH//Fragments",
	"fragTemplatePath": "MFSH//FragmentTemplate.txt",
	"baseUrl": "http://hl7.org/fhir/us/breast-radiology",
	"mfshPaths": [
		"MFSH"
	],
	"cleanDirs": [
		{
			"path": "build\\input\\pagecontent",
			"filter": "StructureDefinition-*-intro.xml"
		},
		{
			"path": "FSH",
			"filter": "*.fsh"
		},
		{
			"path": "GraphFiles",
			"filter": "*.nodeGraph"
		},
		{
			"path": "ApiBuild",
			"filter": "*.*"
		}

	],
	"defines": [
		{
			"name": "%GraphDir%",
			"value": "..\\GraphFiles"
		},
		{
			"name": "%ApiBuildDir%",
			"value": "..\\ApiBuild"
		},
		{
			"name": "%PageContent%",
			"value": "..\\build\\input\\pagecontent"
		}
	]
}